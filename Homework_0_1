using System;

internal class Program
{
    static void Main(string[] args)
    {
        int countPictures = 52;
        int countRows = 3;
        int superMeasurePictures;
        int filledRows;

        filledRows = countPictures / countRows;
        superMeasurePictures = countPictures % countRows;

        Console.WriteLine("Полностью заполненных рядов " + filledRows);
        Console.WriteLine("Картинок сверх меры " + superMeasurePictures);
    }
}

using System;

internal class Program
{
    static void Main(string[] args)
    {
        int countKick;

        Console.WriteLine(" Сколько раз ударить вашего соперника ");
        countKick = Convert.ToInt32(Console.ReadLine());

        while (countKick-- > 1)
        {
            Console.WriteLine(" Удар " + countKick);
        }
        Console.WriteLine(" Ваш противник побеждён!!!");
    }
}
